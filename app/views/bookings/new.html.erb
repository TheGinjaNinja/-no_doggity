<div class = "container">
  <div class= "form-wrapper" style="width: 500px; padding: 50px; margin: 0 auto;">
    <%= simple_form_for [@dog, @booking] do |f| %>
    <h1 class= "title" style= "color: black; ">Hang out with <%= @dog.name%>!</h1>
    <%= f.input :booked_from, label: "Start date", as: :string, required: false, input_html: {class: "datepicker"} %>
    <%= f.input :booked_until, label: "End date", as: :string, required: false, input_html: {class: "datepicker"} %>
    <%= f.submit "Go", class: "btn btn-primary" %>
  <% end %>
</div>
</div>

